
/* 自定义顶部导航 */
.nav{
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav .nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:36rpx;
}
.nav .back{
  width: 52px;
  height: 42px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
  z-index: 11
}
.bg-primary{
  background-color: #ffffff;
}
.bg-white{
  background-color: #ffffff;
}
.bg-gray{
  background-color: #f7f7f7;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}